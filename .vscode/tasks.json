{
	"version": "2.0.0",
	"tasks": [
		{
			"type": "shell",
			"command": "dexterous_developer_cli -e launcher",
			"problemMatcher": [
				"$rustc"
			],
			"label": "Example Launcher"
		},
		{
			"type": "shell",
			"command": "dexterous_developer_cli -e simple_cli",
			"problemMatcher": [
				"$rustc"
			],
			"label": "Example Simple CLI"
		},
		{
			"type": "shell",
			"command": "cargo clippy --all-features --fix --allow-dirty && cargo fmt",
			"label": "Clippy & Fmt"
		},
		{
			"type": "shell",
			"command": "cargo install --path ./dexterous_developer_cli",
			"label": "Install CLI"
		}
	]
	
}